module outline

imports
  
  signatures/Calc-sig
  libspoofax/editor/outline

rules
  
  editor-outline:
    (_, _, ast, path, project-path) -> outline
    where
      outline := <simple-label-outline(to-outline-label)> ast
  
  to-outline-label :
    State(_, name, _) -> $[[name] : State ] 
